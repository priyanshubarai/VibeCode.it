generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  email String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
}
